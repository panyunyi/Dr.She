<% include header %>
    <div id="page-wrapper">
        <div class="container-fluid">
            <div class="row row-title">
                <div class="col-lg-12">
                    <h1 class="page-header">采集</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-4">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            问诊内容采集
                        </div>
                        <div class="panel-body">
                            <p class="text-warning">
                                <%=content%>
                            </p>
                            <p class="text-success">
                                <%=success%>
                            </p>
                            <p class="text-danger">
                                <%=fail%>
                            </p>
                        </div>
                        <div class="panel-footer">
                            <button id="collect" type="button" disabled class="btn btn-default">开始采集</button>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container-fluid -->
    </div>
    <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- jQuery -->
    <script src="../vendor/jquery/jquery.min.js"></script>

    <!-- Bootstrap Core JavaScript-->
    <script src="../vendor/bootstrap/js/bootstrap.min.js"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="../vendor/metisMenu/metisMenu.min.js"></script>

    <!-- Custom Theme JavaScript -->

    <script src="../dist/js/sb-admin-2.js"></script>

    <script src="../dist/js/semantic.min.js"></script>
    <script src="../dist/js/jquery.dataTables.min.js"></script>
    <script src="../dist/js/dataTables.semanticui.min.js"></script>
    <script src="../dist/js/dataTables.buttons.min.js"></script>
    <script src="../dist/js/buttons.semanticui.min.js"></script>
    <script src="../dist/js/dataTables.select.min.js"></script>
    <script src="../dist/js/dataTables.editor.min.js"></script>
    <script src="../dist/js/editor.semanticui.min.js"></script>
    <script>
        if (<%=count %>>0){
            $('#collect').removeAttr('disabled');
            $('#collect').on('click', function () {
                $.get("/admin/collect/go", function (data, status) {
                    alert("已采集: " + data );
                });
            });
        }
    </script>
    </body>

    </html>