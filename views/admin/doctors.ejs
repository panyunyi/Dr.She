<% include header %>
        <div id="page-wrapper">
            <div class="container-fluid">
            <div class="row row-title">
                <div class="col-lg-12">
                    <h1 class="page-header">医生</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            医生数据信息
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <table id="table" class="ui celled table" cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th>序号</th>
                                        <th>姓名</th>
                                        <th>医院</th>
                                        <th>医院等级</th>
                                        <th>医生等级</th>
                                        <th>职称</th>
                                        <th>科室</th>
                                        <th>擅长</th>
                                        <!-- <th>教育背景</th> -->
                                        <th>创建时间</th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container-fluid -->
    </div>
    <!-- /#page-wrapper -->

</div>
<!-- /#wrapper -->

<!-- jQuery -->
<script src="../vendor/jquery/jquery.min.js"></script>

<!-- Bootstrap Core JavaScript-->
<script src="../vendor/bootstrap/js/bootstrap.min.js"></script>

<!-- Metis Menu Plugin JavaScript -->
<script src="../vendor/metisMenu/metisMenu.min.js"></script>

<!-- Custom Theme JavaScript -->

<script src="../dist/js/sb-admin-2.js"></script>

<script src="../dist/js/semantic.min.js"></script>
<script src="../dist/js/jquery.dataTables.min.js"></script>
<script src="../dist/js/dataTables.semanticui.min.js"></script>
<script src="../dist/js/dataTables.buttons.min.js"></script>
<script src="../dist/js/buttons.semanticui.min.js"></script>
<script src="../dist/js/dataTables.select.min.js"></script>
<script src="../dist/js/dataTables.editor.min.js"></script>
<script src="../dist/js/editor.semanticui.min.js"></script>
<script>
    var table = $('#table').DataTable({
                "processing": true,
                stateSave: true,
                "ajax": '../api/json/doctors',
                "columns": [
                    { "data": null },
                    { "data": "name" },
                    { "data": "hospital" },
                    { "data": "hospital_grade" },
                    { "data": "level_title" },
                    { "data": "title" },
                    { "data": "clinic" },
                    { "data": "good_at" },
                    // { "data": "education_background" },
                    { "data": "createdAt" }
                ],
                select: true,
                "order": [[8, 'desc']],
                "language": {
                    "url": "../vendor/zh_CN.txt"
                }
            });
            table.on('order.dt search.dt', function () {
                table.column(0, {
                    search: 'applied',
                    order: 'applied'
                }).nodes().each(function (cell, i) {
                    cell.innerHTML = i + 1;
                });
            }).draw();
</script>
</body>

</html>
